<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" fx:id="rootVistaPrincipal" prefHeight="800.0" prefWidth="1080.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="appacademia.VistaPrincipalController">
   <children>
      <MenuBar layoutY="2.0" prefHeight="25.0" prefWidth="600.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
        <menus>
          <Menu mnemonicParsing="false" text="Curso">
            <items>
                  <MenuItem mnemonicParsing="false" onAction="#onActionMenuItemNuevo" text="Nuevo" />
              <MenuItem mnemonicParsing="false" onAction="#onActionMenuItemMatricula" text="Matricula" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Alumnos">
            <items>
              <MenuItem mnemonicParsing="false" onAction="#onActionMenuItemTablaAlumnos" text="Vista Tabular" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Help">
            <items>
              <MenuItem mnemonicParsing="false" text="About" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
   </children>
</AnchorPane>
